package Lesson2;

public class PrimitiveDataTypes {
    public static void main(String [] args){
    
    byte b1 = 10;
    byte b2 = 20;
    byte b3 = 100;
    
    short s1 = 5;
    short s2 = -10;
    short s3 = 0;
  
    int i1 = 500;
   
    long l1 = 1000;
    long l2 = 1000000000000L;
    long l3 = 51L;
    
    float f1 = 3.14F;
    float f2=2.5F;
    float f3=20.0f;
    
    double d1=5.5;
    double d2=67.65;
    
    char c1= 'a';
    char c2= 'ะช';
    char c3= '1';
    char c4= ' ';
    
    char c5= 1280; // 10
    
    char c6='\u0500'; // 16
    
    boolean bool1=true;
    boolean bool2=false;
    
    
    int a1 = 60;
    int a2 = 0b111100;
    int a3 = 0B111100;
 
    int a4 = 074;
    
    int a5 =0x3C;
    int a6 =0X3C;
    
    
    int a7 = 1_000_0_____0_0;
    
    System.out.println(c5);
    System.out.println(c6);
      
    
   } 
}
